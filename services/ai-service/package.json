{
  "name": "yap-ai-service",
  "version": "1.0.0",
  "private": true,
  "description": "YAP AI Service for GPT and other AI model integrations",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t yap-ai-service:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f ai-service"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "express": "^4.19.2",
    "axios": "^1.6.2",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "ioredis": "^5.3.2",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.20.0",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "gpt",
    "openai",
    "anthropic",
    "claude",
    "chat",
    "language-learning",
    "tokens",
    "microservice"
  ],
  "author": "YAP Technologies",
  "license": "UNLICENSED"
}
